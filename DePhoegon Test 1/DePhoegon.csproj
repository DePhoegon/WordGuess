<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <RootNamespace>DePhoegon</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <StartupObject>DePhoegon.WordGuess</StartupObject>
    <AnalysisLevel>latest</AnalysisLevel>
    <NeutralLanguage>en</NeutralLanguage>
    <AssemblyVersion>1.0</AssemblyVersion>
    <FileVersion>2.5.0</FileVersion>
    <SourceControlInformationFeatureSupported>False</SourceControlInformationFeatureSupported>
    <MSBuildGitHashReplaceInfoVersion>False</MSBuildGitHashReplaceInfoVersion>
    <InformationalVersion>2.5.0 - x86 Native</InformationalVersion>
    <EmbedUntrackedSources>false</EmbedUntrackedSources>
    <PackageTags>game;hangman;dephoegon</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Copyright>All Rights Reserved</Copyright>
    <Product>$(AssemblyName)'s HangMan</Product>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Company>DePhoegon Inc</Company>
    <Authors>DePhoegon</Authors>
    <PlatformTarget>x86</PlatformTarget>
    <PublishTrimmed>False</PublishTrimmed>
    <PublishAot>True</PublishAot>
    <Title>DePhoegon's ManHang</Title>
    <PackageIcon>hangman.png</PackageIcon>
    <IncludeSymbols>False</IncludeSymbols>
    <AssemblyName>$(MSBuildProjectName)s HangMan</AssemblyName>
    <ApplicationIcon>hangman.ico</ApplicationIcon>
    <Description>DePhoegons word guessing game.</Description>
    <PackageId>DePhoegon's HangMan Word Guessing Game.</PackageId>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>full</DebugType>
    <IsPublishable>False</IsPublishable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <IsPublishable>False</IsPublishable>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="hangman.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\hangman.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>
